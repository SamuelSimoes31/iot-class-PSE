

void pulseIR(long microsegs) // Utilização dos interrupts Cli() e Sei()
{
  cli();  
    while (microsegs > 0) 
    {
    digitalWrite(LedIR, HIGH);  
    delayMicroseconds(10);       
    digitalWrite(LedIR, LOW);   
    delayMicroseconds(10);        
    microsegs -= 26; 
    }
  sei(); 
}

